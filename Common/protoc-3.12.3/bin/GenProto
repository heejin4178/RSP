#!/bin/bash

# Protocol.cs 파일을 클라이언트와 서버에 복사
cp /Users/heejinkim/RSP/Common/protoc-3.12.3/bin/Protocol.cs /Users/heejinkim/RSP/Client/Assets/@Scripts/Packet
cp /Users/heejinkim/RSP/Common/protoc-3.12.3/bin/Protocol.cs /Users/heejinkim/RSP/Server/Server/Packet

# ClientPacketManager.cs와 ServerPacketManager.cs 파일을 각각 클라이언트와 서버에 복사
cp /Users/heejinkim/RSP/Server/PacketGenerator/bin/ClientPacketManager.cs /Users/heejinkim/RSP/Client/Assets/@Scripts/Packet
cp /Users/heejinkim/RSP/Server/PacketGenerator/bin/ServerPacketManager.cs /Users/heejinkim/RSP/Server/Server/Packet